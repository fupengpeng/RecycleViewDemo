<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fpp.recycleviewdemo">

    <application
        android:name=".base.app.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".recycleview.RecyclerViewActivity" />
        <activity android:name=".recycleview.LVOrRCVActivity" />
        <activity android:name=".recycleview.RcyclerViewHorizontalActivity" />
        <activity android:name=".ui.rvone.RVOneActivity" />
        <activity android:name=".ui.rvtwo.RVTwoActivity" />
        <activity android:name=".listview.XListViewActivity" />
        <activity android:name=".listview.ClassifyActivity"></activity>
    </application>

</manifest>